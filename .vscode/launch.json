// A launch configuration that compiles the extension and then opens it inside a new window
// Use IntelliSense to learn about possible attributes.
// Hover to view descriptions of existing attributes.
// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: current file",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceRoot}/notebooks/titanic_notebooks",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug python Script",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/analyzer.py",
      "args": [
        ".\\notebooks\\titanic_notebooks\\a-data-science-framework-to-achieve-99-accuracy.ipynb"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Debug JS Script",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/instrumenter.js",
      "args": [
        ".\\notebooks\\titanic_notebooks\\titanic-eda-model-pipeline-keras-nn.py"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Debug Tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/jest",
      "args": [
        "--runInBand"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Debug Current Test File",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/.bin/jest",
      "args": [
        "${file}"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}